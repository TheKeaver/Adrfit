<UI>
    <Widgets>
        <NinePatchImage
            halign="center"
            valign="center"
            horizontal="0"
            vertical="0"
            width="50%"
            height="50%"

            image="texture_ui_button_pressed"
            thickness="4,4,4,4"

            aspect-ratio="1.0"

            id="foo"
            />
        <Label
            halign="center"
            valign="center"
            horizontal="0"
            vertical="0"
            width="50%"
            height="50%"

            content="Hello XML!"
            font="font_game_over"
            />
        <Panel
            halign="center"
            valign="center"
            horizontal="0"
            vertical="0"
            width="500"
            height="500">
            <Label
                halign="left"
                valign="bottom"
                horizontal="0"
                vertical="0"
                width="100"
                height="100"
                content="Left!"
                font="font_game_over"
                />
        </Panel>
        <Image
            halign="center"
            valign="center"
            horizontal="0"
            vertical="0"
            width="300"
            height="100"
            image="texture_explosion"
            />
        <Label
            halign="right"
            valign="bottom"
            horizontal="0"
            vertical="0"
            width="100"
            height="100"
            content="Blink!"
            font="font_game_over"
            id="label_blink"
            />
        <Button
            halign="left"
            valign="bottom"
            horizontal="10"
            vertical="10"
            width="200"
            height="50"
            released-image="texture_ui_button_released"
            released-thickness="4,4,4,4"
            hover-image="texture_ui_button_hover"
            hover-thickness="4,4,4,4"
            pressed-image="texture_ui_button_pressed"
            pressed-thickness="4,4,4,4"
            onclick="queue:UI.TestButtonPressedEvent">
            <NinePatchImage
                halign="center"
                valign="center"
                horizontal="0"
                vertical="0"
                width="25"
                height="25"
                image="texture_ui_button_pressed"
                thickness="4,4,4,4"
                />
        </Button>
    </Widgets>
</UI>
